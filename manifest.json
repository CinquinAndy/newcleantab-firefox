{
	"manifest_version": 2,
	"name": "New Clean Tab",
	"version": "1.0",
	"description": "A simple clean new tab page that redirects to your homepage with a clean URL",
	"icons": {
		"48": "icon48.png",
		"96": "icon96.png"
	},
	"permissions": [
		"browserSettings",
		"tabs",
		"activeTab",
		"webNavigation",
		"storage",
		"<all_urls>"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"chrome_url_overrides": { "newtab": "newcleantab.html" }
}
